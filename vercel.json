{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "node scripts/prepare-vercel-deploy.js && cd apps/web && npm install && npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "apps/web/.next",
  "rewrites": [{ "source": "/(.*)", "destination": "/apps/web/$1" }],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/web/"
}
